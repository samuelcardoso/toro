{
  "name": "stocklistener-back",
  "version": "1.0.0",
  "private": true,
  "authors": [
    "Samuel Cardoso"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
    "start": "node ./node_modules/.bin/serverless offline --noTimeout --host 0.0.0.0",
    "socket": "node .build/src/websocket/main.js",
    "debug": "run-script build && NODE_ENV=dev DEBUG=* node --inspect ./node_modules/.bin/serverless offline --noTimeout",
    "clean": "rimraf .build",
    "lint": "eslint ./app/**/*.ts ./test/**/*.ts",
    "format": "tsfmt -r",
    "prepublish": "npm run build",
    "pretest": "cross-env npm build && npm run lint",
    "test": "cross-env mocha",
    "tsc": "tsc"
  },
  "dependencies": {
    "redis": "^2.8.0",
    "ws": "^7.2.1"
  },
  "devDependencies": {
    "@types/node": "^11.10.4",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "chai": "latest",
    "chai-as-promised": "^7.1.1",
    "chai-http": "latest",
    "cross-env": "latest",
    "eslint": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "latest",
    "nsrun": "^1.4.1",
    "nyc": "latest",
    "rimraf": "latest",
    "serverless": "^1.43.0",
    "serverless-offline": "^3.18.0",
    "serverless-plugin-typescript": "^1.1.5",
    "serverless-prune-plugin": "^1.3.2",
    "sinon": "^7.2.4",
    "typescript": "^3.5.3"
  }
}
